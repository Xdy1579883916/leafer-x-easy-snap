{
  "name": "leafer-x-easy-snap",
  "type": "module",
  "version": "1.10.1",
  "packageManager": "pnpm@10.14.0",
  "description": "leafer 吸附插件，为 Leafer 应用提供元素移动自动吸附、对齐辅助线功能",
  "author": "XiaDeYu <1579883916@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/Xdy1579883916/leafer-x-easy-snap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xdy1579883916/leafer-x-easy-snap.git"
  },
  "bugs": "https://github.com/Xdy1579883916/leafer-x-easy-snap/issues",
  "keywords": [
    "leafer",
    "吸附插件",
    "对齐辅助线"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm run -C playground dev",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "prepare": "simple-git-hooks"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@antfu/ni": "catalog:cli",
    "@antfu/utils": "catalog:inlined",
    "@cr/lib": "workspace:*",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "eslint": "catalog:cli",
    "lint-staged": "catalog:cli",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:utils",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "unbuild": "catalog:cli",
    "vite": "catalog:cli",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
